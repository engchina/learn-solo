{
  "name": "learn-solo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "nodemon",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "electron:dev": "vite",
    "electron:build": "tsc -b && vite build && electron-builder",
    "electron:build:win": "tsc -b && vite build && electron-builder --win",
    "electron:build:mac": "tsc -b && vite build && electron-builder --mac",
    "electron:build:linux": "tsc -b && vite build && electron-builder --linux",
    "dist": "npm run electron:build",
    "dist:win": "npm run electron:build:win",
    "dist:dir": "npm run build && electron-builder --dir",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@monaco-editor/react": "^4.6.0",
    "@tailwindcss/typography": "^0.5.16",
    "antd": "^5.21.0",
    "chokidar": "^4.0.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "katex": "^0.16.11",
    "lucide-react": "^0.511.0",
    "monaco-editor": "^0.52.0",
    "multer": "^2.0.2",
    "prismjs": "^1.29.0",
    "puppeteer": "^23.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.3.0",
    "rehype": "^13.0.2",
    "rehype-katex": "^7.0.1",
    "rehype-prism-plus": "^2.0.1",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tailwind-merge": "^3.0.2",
    "unified": "^11.0.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/katex": "^0.16.7",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.15.30",
    "@types/prismjs": "^1.26.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vercel/node": "^5.3.6",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^9.2.0",
    "electron": "^28.3.3",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
